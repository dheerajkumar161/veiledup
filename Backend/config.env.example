# Backend Configuration
PORT=5000
MONGODB_URI=mongodb+srv://your-username:your-password@your-cluster.mongodb.net/

# Testing Configuration
TEST_CONCURRENT_USERS=100
TEST_API_CALLS_PER_USER=200
TEST_SOCKET_MESSAGES_PER_USER=100
TEST_TIMEOUT=10000
TEST_DURATION=60

# Performance Thresholds
THRESHOLD_SUCCESS_RATE=85
THRESHOLD_RESPONSE_TIME=500
THRESHOLD_THROUGHPUT=100

# Socket Testing
SOCKET_TIMEOUT=5000
SOCKET_RETRY_ATTEMPTS=3

# Load Testing Levels
LOAD_LEVEL_BASELINE_USERS=25
LOAD_LEVEL_MEDIUM_USERS=50
LOAD_LEVEL_HIGH_USERS=100
LOAD_LEVEL_EXTREME_USERS=200
LOAD_LEVEL_ULTIMATE_USERS=500

# Environment
NODE_ENV=development
LOG_LEVEL=info 